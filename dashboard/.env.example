# Firebase (from Firebase Console > Project Settings > Your apps > Web)
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_SENDER_ID=your_messaging_sender_id
VITE_FIREBASE_APP_ID=your_firebase_app_id

# Schwab / ThinkorSwim API (FREE with your brokerage account)
# Get credentials at: https://developer.schwab.com
SCHWAB_CLIENT_ID=your_client_id_here
SCHWAB_CLIENT_SECRET=your_client_secret_here
SCHWAB_REDIRECT_URI=http://localhost:3001/callback

# OpenAI API (for AI-powered analysis)
# Get key at: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_key_here
